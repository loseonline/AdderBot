{
    "name": "Basic Bots Scrapper",
    "description": "Telegram'da üye çekmek için kullanışlı bot.",
    "logo": "https://telegra.ph/file/f151dcfb4f8b25e8b4261.jpg",
    "keywords": [
      "telegram",
      "bot",
      "python",
      "pyrogram",
      "scrapper"
    ],
      "buildpacks": [{
      "url": "heroku/python"
    }],
    "formation": {
      "worker": {
        "quantity": 1,
        "size": "free"
      }
    },
    "addons": [
        {
           "options": {
              "version": "12"
           },
           "plan": "heroku-postgresql"
        }
     ],
    "repository": "https://github.com/usermusti",
    "env": {
      "APP_ID": {
        "description": "my.telegram.org.",
        "required": true,
        "value": ""
      },
      "API_HASH": {
        "description": "my.telegram.org.",
        "required": true,
        "value": ""
      },
      "BOT_TOKEN": {
        "description": "@BotFather",
        "required": true,
        "value": ""
      },
      "UPDATES_CHANNEL": {
        "description": "Güncelleme kanalı kullanıcı adı.",
        "required": true,
        "value": ""
      },
      "OWNER": {
        "description": " Sahip ID.",
        "required": true,
        "value": ""
      },
      "PREMIUM": {
        "description": "Premium kullanıcı ID",
        "required": true,
        "value": ""
      },
      "OWNER_USERNAME": {
        "description": "Sahip Hesap Kullanıcı Adı - '@' olmadan! ",
        "required": true,
        "value": ""
      },
      "LOG_CHANNEL": {
        "description": "Log kanalı ID",
        "required": true,
        "value": ""
      }
    }
  }
